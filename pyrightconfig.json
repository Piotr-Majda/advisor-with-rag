{
    "include": [
        "services",
        "shared"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        "**/.*"
    ],
    "ignore": [
        "**/.venv"
    ],
    "defineConstant": {
        "DEBUG": true
    },
    "venvPath": ".",
    "reportMissingImports": true,
    "reportMissingTypeStubs": false,
    "pythonVersion": "3.11",
    "pythonPlatform": "Linux"
}